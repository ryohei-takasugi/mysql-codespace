{
  "name": "learn-rdbms",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "mysql",
  "customizations": {
    "vscode": {
      "extensions": [
        "cweijan.vscode-database-client2"
      ],
      "settings": {
        "sqltools.connections": [
          {
            "name": "Local MySQL (Codespace)",
            "driver": "MySQL",
            "server": "127.0.0.1",
            "port": 3306,
            "database": "mydatabase",
            "username": "myuser",
            "password": "mypassword"
          }
        ]
      }
    }
  }
}